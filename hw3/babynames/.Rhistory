leapYear <- function(year) {
if (year != as.integer(year)) {
stop("year is not a whole number")
} else if (year <= 0) {
stop("year must be positive")
} else {
return(year %% 4 == 0 || (year %% 100 == 0 && year %% 400 != 0))
}
}
